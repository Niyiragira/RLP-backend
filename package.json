{
  "name": "rlp-backend",
  "version": "1.0.0",
  "description": "Backend of RLP-project",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node ./src/app.js",
    "build": "babel src --out-dir dist --source-maps inline --copy-files",
    "start": "npm run build && node --require @babel/polyfill ./dist/app.js",
    "migrate": "npx sequelize-cli db:migrate",
    "seeding": "npx sequelize-cli db:seed:all",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "migrateDB": "npx sequelize-cli db:migrate:undo:all && npm run migrate && npm run seeding"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Niyiragira/RLP-backend.git"
  },
  "author": "Niyiragira",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Niyiragira/RLP-backend/issues"
  },
  "homepage": "https://github.com/Niyiragira/RLP-backend#readme",
  "dependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.15",
    "@babel/register": "^7.13.14",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
